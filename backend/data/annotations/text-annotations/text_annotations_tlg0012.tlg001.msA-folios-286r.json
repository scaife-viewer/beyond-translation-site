  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:22.49",
    "lemma": "σημειῶδες",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol6942",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA286RN_0456@0.6398,0.2746,0.1877,0.05297"
