  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:11.146",
    "lemma": "οἵ τε σε πεφρίκασι :",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol4666",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA145RN_0317@0.142,0.1103,0.642,0.0345"
