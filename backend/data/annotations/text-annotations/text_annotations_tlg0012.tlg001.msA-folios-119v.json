    ]
  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msAim.hmt:9.393",
    "comment": "γράφεται Ζηνοδ  νήου Ἀπολλω",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msAim_hmt_9_393",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA119VN_0622@0.4455,0.6028,0.06393,0.03154"
