    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol2378",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA078RN_0079@0.510316875460575,0.717842323651452,0.335298452468681,0.0381742738589212"
