  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:15.34",
    "lemma": "‡ ἡ δὲ γέλασσε χειλεσιν ⁚",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol5696",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA193RN_0364@0.141,0.0984,0.638,0.0466"
