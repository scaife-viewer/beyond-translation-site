  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:4.478",
    "lemma": "ἱππῆας μὲν πρῶτα·",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol2112",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA057RN_0058@0.61790715,0.46514523,0.18975682,0.11092669"
