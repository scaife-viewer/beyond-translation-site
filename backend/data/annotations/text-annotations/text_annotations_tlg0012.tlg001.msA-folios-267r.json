  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:20.267r_2",
    "lemma": "τόσσους δ' ἀνους",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol7703",
