[
  {
    "urn": "urn:cts:greekLit:tlg5026.msAint.hmt:2.801",
    "comment": "ὅτι Ζηνοδοτος γράφει ἐξ Ενέτης ⁑",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol1614",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA041RN_0042@0.0743,0.438,0.0477,0.0333"
