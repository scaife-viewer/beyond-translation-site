    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msA_hmt_9_187",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA115RN_0287@0.1739,0.1198,0.6625,0.04260"
