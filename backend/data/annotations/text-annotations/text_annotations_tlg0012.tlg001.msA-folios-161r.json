  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:12.H40",
    "lemma": "καί οι Τεῦκρος ἅμ' επέσθω·",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol5094",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA161RN_0332@0.1663,0.7404,0.5961,0.0332"
    },
    "references": [
      "urn:cts:greekLit:tlg0012.tlg001.msA-folios:161r.12.350"
