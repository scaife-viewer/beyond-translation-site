  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:9.335",
    "lemma": "‡θυμαρέα :",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msA_hmt_9_335",
