  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:11.214",
    "lemma": "‡ ῆπαρ ὑπο πραπίδων :",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol4720",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA149RN_0321@0.158,0.093,0.57,0.0233"
