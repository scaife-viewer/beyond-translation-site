  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:16.217r1",
    "lemma": "οἷοί περ πάρος ῆτε μετ' ἀνδράσι .",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol7944",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA217RN_0388@0.6268,0.2833,0.1728,0.1318"
