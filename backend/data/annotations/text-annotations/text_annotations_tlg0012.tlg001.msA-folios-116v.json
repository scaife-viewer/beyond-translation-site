    ]
  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msAil.hmt:9.289",
    "comment": "πεπωμένον",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msAil_hmt_9_289",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA116VN_0619@0.7798,0.4084,0.0587,0.0131"
