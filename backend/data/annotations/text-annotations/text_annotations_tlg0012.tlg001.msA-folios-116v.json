    ]
  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msAim.hmt:9.281",
    "comment": "βραχ  διασταλτ  ἐπι το ανερας .",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msAim_hmt_9_281",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA116VN_0619@0.4251,0.2959,0.0546,0.0267"
