    ]
  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msAint.hmt:9.287",
    "comment": "σημειοῦνται τινὲς ἀντι τοῦ μαλερῶ πυρι ⁑",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msAint_hmt_9_287",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA116VN_0619@0.8935,0.3519,0.0302,0.0436"
