    ]
  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msAim.hmt:9.282",
    "comment": "Ἀριστ  ἐμπλήσειν ⁑",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msAim_hmt_9_282",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA116VN_0619@0.4095,0.3477,0.08861,0.04066"
    },
    "references": [
