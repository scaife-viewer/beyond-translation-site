    ]
  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msAim.hmt:9.283",
    "comment": "διασταλτέον βραχ  φρένας θεοί ⁑",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msAim_hmt_9_283",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA116VN_0619@0.4187,0.3923,0.0596,0.0343"
    },
    "references": [
      "urn:cts:greekLit:tlg0012.tlg001.msA-folios:116v.9.244"
    ]
  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msAim.hmt:9.284",
    "comment": "σημειοῦνται τινες ὅτι τὴν όλην Πελοπόννησον : οὐκ οῖδεν ὁ ποιητ  Ἡσιοδ  δὲ ⁑",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msAim_hmt_9_284",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA116VN_0619@0.4159,0.4276,0.0611,0.0559"
    },
    "references": [
      "urn:cts:greekLit:tlg0012.tlg001.msA-folios:116v.9.247"
    ]
  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msAim.hmt:9.285",
    "comment": "σημειοῦνται τινὲς οτ  παρεῖται τὸ τάδε λέγων ⁑",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msAim_hmt_9_285",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA116VN_0619@0.4203,0.5804,0.0522,0.0562"
