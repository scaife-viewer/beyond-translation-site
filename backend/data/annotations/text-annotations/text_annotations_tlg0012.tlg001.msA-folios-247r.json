  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:18.142",
    "lemma": "πέλωρ αίητον⁚",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol6469",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA247RN_0418@0.616,0.3306,0.226,0.2757"
