  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:10.4609",
    "lemma": "οὐδ' ἀλαὸσσκοπιὴν :",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol4141",
