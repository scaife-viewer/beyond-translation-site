  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:9.662",
    "lemma": "μῦθον ἀγασσάμενοι",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msA_hmt_9_662",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA125VN_0628@0.1912,0.1089,0.7183,0.03638"
