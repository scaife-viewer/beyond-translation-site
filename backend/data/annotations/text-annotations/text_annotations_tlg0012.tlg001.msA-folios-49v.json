    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol1917",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA049VN_0551@0.23728814,0.73969571,0.64627856,0.05864454"
