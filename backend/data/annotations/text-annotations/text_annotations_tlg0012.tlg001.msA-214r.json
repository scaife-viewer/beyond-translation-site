[
  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:16.214r1",
    "lemma": "δεύτερον ορμηθείς .",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol8115",
