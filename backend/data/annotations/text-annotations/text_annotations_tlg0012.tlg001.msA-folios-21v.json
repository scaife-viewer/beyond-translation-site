  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:1.1231",
    "lemma": "αὐτὰρ ὃ μήνιε",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol205",
