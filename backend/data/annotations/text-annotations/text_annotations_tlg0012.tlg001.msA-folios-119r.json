  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msAil.hmt:9.373",
    "comment": "πόλις τῆς βοωτιας",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msAil_hmt_9_373",
