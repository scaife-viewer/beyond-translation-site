  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msAil.hmt:9.153",
    "comment": "περιεποιήσαντο :",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msAil_hmt_9_153",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA114RN_0286@0.2744,0.5586,0.1039,0.0131"
