  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:10.1500",
    "lemma": "‡ ἐν φυλάκεσσ'ἵνα γάρ σφιν",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol3866",
