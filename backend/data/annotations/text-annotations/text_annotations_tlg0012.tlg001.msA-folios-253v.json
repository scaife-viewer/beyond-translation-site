  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:19.2015office10_1",
    "lemma": "‡ τῶν ἀνδρῶν γενεῆς⁚",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol6631",
