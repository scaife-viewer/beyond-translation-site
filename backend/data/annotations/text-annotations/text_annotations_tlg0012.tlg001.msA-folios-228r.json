[
  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:17.98",
    "lemma": "‡ τῶ τις νῦν ἰθὺς",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol6092",
