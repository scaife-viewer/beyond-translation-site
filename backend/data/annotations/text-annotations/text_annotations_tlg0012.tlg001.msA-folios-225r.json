  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:17.36",
    "lemma": "‡ Πατρόκλῳ περιβὰς ,",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol6030",
