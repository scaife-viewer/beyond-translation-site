    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:13.A5",
    "lemma": "πριαμίδης μὲν ἔπειτα κατα στῆθος",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol8315",
