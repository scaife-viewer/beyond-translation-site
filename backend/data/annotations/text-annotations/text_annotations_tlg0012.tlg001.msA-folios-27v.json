  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:2.272",
    "lemma": "Ἀργείην Ἑλένην ·",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol1226",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA027VN_0529@0.215,0.1026,0.6342,0.0476"
