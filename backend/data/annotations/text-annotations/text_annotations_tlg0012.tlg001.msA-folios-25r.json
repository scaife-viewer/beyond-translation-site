  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:2.54",
    "lemma": "Τὰ φρονέοντ' ἀναθυμον·",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol1184",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA025RN_0026@0.1717,0.0948,0.5463,0.0207"
