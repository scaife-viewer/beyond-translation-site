  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:12.H10",
    "lemma": "ὑψιπέτης :",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol5078",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA158RN_0329@0.1692,0.6979,0.6216,0.0785"
