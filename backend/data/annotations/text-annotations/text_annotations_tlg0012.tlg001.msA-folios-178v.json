    ]
  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:13.A39",
    "lemma": "ῆ ρα καὶ ὠρμήθη ὄρεϊ νιφόεντι",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol8352",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA178VN_0680@0.204,0.4628,0.233,0.0398"
