  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:23.400",
    "lemma": "ἦλθε δ' ἐπι ψυχῆ",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol7120",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA294RN_0464@0.621,0.2838,0.215,0.1284"
