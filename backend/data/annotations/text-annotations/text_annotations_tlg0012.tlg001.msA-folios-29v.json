[
  {
    "urn": "urn:cts:greekLit:tlg5026.msAint.hmt:2.440",
    "comment": "ὅτι αντὶ πλήσσων τὸ παθητ παθητικὸν παρειληφεν ⁑",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol1595",
