    "lemma": "παραβλῶπες ,",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msA_hmt_9_474",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA121VN_0624@0.2079,0.6147,0.2334,0.1231"
