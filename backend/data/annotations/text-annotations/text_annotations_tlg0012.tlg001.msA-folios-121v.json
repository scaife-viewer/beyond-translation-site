  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:9.471",
    "lemma": "στρεπτὸς δέ τε :",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:tlg5026_msA_hmt_9_471",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA121VN_0624@0.2222,0.142,0.6687,0.0541"
