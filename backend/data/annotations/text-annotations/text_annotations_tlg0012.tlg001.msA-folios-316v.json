  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:24.38",
    "lemma": "κῆρυξ καὶ Πρίαμος :",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol7406",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA316VN_0818@0.224,0.1096,0.64,0.0353"
