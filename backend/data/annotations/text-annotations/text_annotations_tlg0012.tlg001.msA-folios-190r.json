  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:14.F4",
    "lemma": "γνωτὸν ἐνι μεγάροις .",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol5285",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA190RN_0361@0.6008,0.3109,0.1962,0.06902"
