  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:22.88",
    "lemma": "ὦ φίλοι Ἀργείων ἡγήτορες ,",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol6981",
