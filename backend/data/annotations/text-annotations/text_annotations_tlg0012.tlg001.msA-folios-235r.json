    },
    "references": []
  },
  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:17.117",
    "lemma": "μαρμαρέην Ἴδην δὲ ,",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol6111",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA235RN_0406@0.637,0.4899,0.178,0.0646"
